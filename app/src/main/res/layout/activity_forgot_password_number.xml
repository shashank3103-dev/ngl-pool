<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/_bg__sign_in_ek1_shape"
    tools:context=".sign_in">


    <RelativeLayout android:id="@+id/content_container"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" >
    </RelativeLayout>
    <RelativeLayout android:id="@+id/group_47593"
        android:layout_marginTop="71dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        >
        <RelativeLayout android:id="@+id/group_6"
            android:layout_marginTop="0dp"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >



            <TextView android:id="@+id/forget_password_"
                android:textStyle="bold"
                android:layout_gravity="center_horizontal|center"
                android:gravity="center"
                android:textSize="20sp"
                android:textColor="@color/forget_password__color"
                android:layout_marginLeft="-3dp"
                android:layout_marginTop="0dp"
                android:layout_width="184dp"
                android:layout_height="31dp"
                android:text="Forgot Password"
                />

        </RelativeLayout>
        <RelativeLayout android:id="@+id/group_33642"
            android:layout_marginTop="55dp"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >


            <TextView android:id="@+id/we_sent_a_password_reset_link_to_kel_____gmail_com__click_on_the_link_in_your_email_to_reset_your_password_"
                android:layout_gravity="center_horizontal|center"
                android:gravity="center"
                android:alpha="0.8"
                android:textSize="12sp"
                android:textColor="@color/forget_password__color"
                android:layout_marginLeft="-2dp"
                android:layout_marginTop="0dp"
                android:layout_width="310.2dp"
                android:layout_height="33.2dp"
                android:text="@string/we_sent_a_password_reset_link_to_kel_____gmail_com__click_on_the_link_in_your_number_to_reset_your_password__string"
                />


            <RelativeLayout android:id="@+id/sample_1"
                android:layout_marginTop="80dp"
                android:layout_centerHorizontal="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >



                <ImageView android:id="@+id/img_31c930894bd4_1_1"
                    android:src="@drawable/img"
                    android:contentDescription="img_31c930894bd4_1_1"
                    android:layout_marginStart="11dp"
                    android:layout_marginTop="8dp"
                    android:layout_width="212dp"
                    android:layout_height="233dp" />

            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout android:id="@+id/group_47694"
            android:layout_marginTop="400dp"
            android:layout_centerHorizontal="true"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            >

            <RelativeLayout android:id="@+id/button"
                android:layout_marginTop="0dp"
                android:layout_marginStart="0dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                >

                <Button   android:id="@+id/proceedToLogin"
                    android:background="@drawable/_bg__button_ek1_shape"
                    android:layout_marginStart="0dp"
                    android:layout_marginTop="0dp"
                    android:text="Proceed to Login"
                    android:textAllCaps="false"
                    android:textColor="@color/white"
                    android:textStyle="bold"
                    android:layout_width="327dp"
                    android:layout_height="56dp" />



            </RelativeLayout>

        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>